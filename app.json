{
  "expo": {
    "name": "UrbanSitio",
    "slug": "aula1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/frutiger_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/frutiger_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dhavvi.aula1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/frutigericon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/frutigericon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b0b1f0c9-3813-4462-be85-e4f43ff0fc8a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b0b1f0c9-3813-4462-be85-e4f43ff0fc8a"
    }
  }
}
